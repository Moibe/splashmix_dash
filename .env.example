# Environment variables for Splashmix Dash (Vite + Svelte)
# Copy to .env and fill with your own values.

# Firebase
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=000000000000
VITE_FIREBASE_APP_ID=1:000000000000:web:abcdef1234567890
VITE_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# Hugging Face token (used for Gradio Space auth)
VITE_HF_TOKEN=hf_your_token

# Optional: OpenAI â€” enable prompt classification in toast
VITE_OPENAI_API_KEY=sk_your_openai_key